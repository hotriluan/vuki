{
  "name": "shoe-store-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/sync-blog.mjs && node scripts/build-unified-search-index.mjs",
    "dev": "next dev",
    "dev:debug": "cross-env PORT=3001 DEBUG=next:* next dev",
    "prebuild": "node scripts/sync-blog.mjs && node scripts/build-unified-search-index.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --coverage --exclude **/*.integration.test.ts",
    "test:integration": "vitest run \"src/**/*.integration.test.ts\"",
    "ci": "next lint && vitest run --coverage --run && next build",
    "db:check": "node scripts/check-db.mjs",
    "analyze": "cross-env ANALYZE=1 next build",
    "release": "semantic-release",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "db:seed": "node scripts/seed.mjs"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.16.2",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "embla-carousel": "8.1.6",
    "embla-carousel-react": "8.1.6",
    "fuse.js": "7.0.0",
    "gray-matter": "^4.0.3",
    "marked": "^12.0.2",
    "next": "14.2.33",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "pino": "^9.12.0",
    "pino-pretty": "^13.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "uuid": "^13.0.0",
    "web-vitals": "4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@next/bundle-analyzer": "^15.5.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.6",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.0",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.24",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "10.4.19",
    "cross-env": "^10.0.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.33",
    "jsdom": "24.0.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prisma": "^6.16.2",
    "semantic-release": "22.0.12",
    "tailwindcss": "3.4.9",
    "typescript": "5.4.5",
    "vitest": "^3.2.4",
    "zod": "3.23.8"
  }
}
